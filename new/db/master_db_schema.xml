<?xml version="1.0" encoding="UTF-8"?>
<re_xml>
	<user_profile>
			<id index='no' type="BIGINT NOT NULL AUTO_INCREMENT"></id>
			<username index='no' type="VARCHAR(100) NOT NULL"></username>
			<phone index='no' type="VARCHAR(15) NOT NULL"></phone>
			<name index='no' type="VARCHAR(90)"></name>
			<industry index='no' type="VARCHAR(50)"></industry>
			<status index='no' type="VARCHAR(45)"></status><!-- active/suspended/archived -->
			<dbname index='no' type="VARCHAR(100)"></dbname>
			<email index='no' type="VARCHAR(100)"></email>
	</user_profile>
	<api_key_mapping>
			<id index='no' type="BIGINT NOT NULL AUTO_INCREMENT"></id>
			<api_key index='no' type="VARCHAR(100) NOT NULL"></api_key>
			<username index='no' type="VARCHAR(100) NOT NULL"></username>
			<status index='no' type="VARCHAR(45)"></status><!-- active/suspended -->
			<dbname index='no' type="VARCHAR(100)"></dbname>
			<call_limit index='no' type="DECIMAL(12,2)"></call_limit>
			<data_stores index='no' type="TEXT"></data_stores>
			<request_types index='no' type="TEXT"></request_types>
			<unique_fields index='no' type="TEXT"></unique_fields>
			<required_fields index='no' type="TEXT"></required_fields>
			<access_control index='no' type="TEXT"></access_control>
	</api_key_mapping>
</re_xml>
